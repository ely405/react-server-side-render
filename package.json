{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && npm run build-client && npm run build-server",
    "build-client": "SET NODE_ENV=production SET BABEL_ENV=production node -r babel-register node_modules/.bin/webpack --progress --display-modules --config webpack/webpack.config.client.js",
    "build-server": "SET NODE_ENV=production SET BABEL_ENV=production node -r babel-register node_modules/.bin/webpack --progress --config webpack/webpack.config.server.js",
    "clean": "rimraf dist/ && rimraf public/app",
    "lint": "eslint src",
    "postmerge": "npm install",
    "postrewrite": "npm install",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "start": "npm run clean & babel-node src/server SET NODE_ENV=development SET BABEL_ENV=development",
    "start-analyzer": "npm run clean && SET NODE_ENV=development SET BABEL_ENV=development ANALYZER=true babel-node src/server",
    "start-production": "npm run build && SET NODE_ENV=production SET BABEL_ENV=production babel-node src/server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-client": "webpack --watch --config webpack/webpack.config.client.js",
    "watch-server": "webpack --watch --config webpack/webpack.config.server.js"
  },
  "keywords": [
    "react",
    "express",
    "redux",
    "webpack"
  ],
  "author": "ely405",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-react": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "husky": "^0.14.3",
    "node-sass": "^4.7.2",
    "react-hot-loader": "^4.0.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "4.1.1",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
